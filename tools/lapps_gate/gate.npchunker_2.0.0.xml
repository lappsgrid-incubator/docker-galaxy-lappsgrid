<tool id="gate.npchunker_2.0.0" name="GATE NounPhraseChunker v2.0.0" version="2.0.0">
	<description>
		GATE Noun Phrase Chunker
	</description>
	<command interpreter="lsd">
		invoke.lsd gate.npchunker_2.0.0 $input $username $password $output
	</command>
	<inputs>
		<param name="input" type="data" format="gate" label="input"/>
		<param name="username" type="text" value="tester" label="Username"/>
		<param name="password" type="text" value="tester" label="Password"/>
	</inputs>
	<outputs>
		<data name="output" format="gate" label="Noun Chunks (GATE)"/>
	</outputs>
	<help>
		GATE Noun Phrase Chunker
	</help>
</tool>
